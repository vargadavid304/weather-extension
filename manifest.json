{
    "name":"Weather Extension",
    "version": "0.1.0",
    "manifest_version": 2,
    "background":{
        "page":"./background.html"
    },
    "browser_action":{},
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    },
    "permissions":[
        "storage"
    ],
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js":[
                "content-script.js"
            ]
        }
    ]
}